{
  "$schema": "http://json-schema.org/schema",
  "id": "CheatSheetSchematics",
  "title": "Schematics for our cheat sheet",
  "type": "object",
  "description": "Generate a default operator documentation",
  "properties": {
    "operator": {
      "type": "string",
      "description": "The name of the operator",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "required": ["operator"]
}
